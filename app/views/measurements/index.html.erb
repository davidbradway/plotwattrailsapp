<h1>Listing measurements</h1>

<table>
  <thead>
    <tr>
      <th>Alwayson</th>
      <th>Heatingac</th>
      <th>Refrigeration</th>
      <th>Dryer</th>
      <th>Cooking</th>
      <th>Other</th>
      <th>Datetimemidpoint</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @measurements.each do |measurement| %>
      <tr>
        <td><%= measurement.AlwaysOn %></td>
        <td><%= measurement.HeatingAC %></td>
        <td><%= measurement.Refrigeration %></td>
        <td><%= measurement.Dryer %></td>
        <td><%= measurement.Cooking %></td>
        <td><%= measurement.Other %></td>
        <td><%= measurement.DatetimeMidpoint %></td>
        <td><%= link_to 'Show', measurement %></td>
        <td><%= link_to 'Edit', edit_measurement_path(measurement) %></td>
        <td><%= link_to 'Destroy', measurement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

